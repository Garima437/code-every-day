class Solution {
    public int centeredSubarrays(int[] nums) {
        int n=nums.length;
        int ans=0;
        for(int i=0;i<n;i++){
            int sum=0;
            HashSet<Integer> st=new HashSet<>();
            for(int j=i;j<n;j++){
                sum+=nums[j];
                st.add(nums[j]);
                if(st.contains(sum)){
                    ans++;
                }
            }
        }
        return ans;
    }
}
