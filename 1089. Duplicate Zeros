class Solution {
    public void duplicateZeros(int[] arr) {
        int i=arr.length-1;
        int x=0;
        for(int z:arr){
            if(z==0) x++;
        }
        int j=arr.length+x-1;
        while(i!=j){
            insert(arr,i,j);
            j--;
            if(arr[i]==0){
                insert(arr,i,j);
                j--;
            }
            i--;
        }
    }
      private void insert(int[] arr,int i,int j){
        if(j<arr.length){
            arr[j]=arr[i];
        }
    }
}
