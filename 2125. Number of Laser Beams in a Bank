class Solution {
    public int numberOfBeams(String[] bank) {
        int n=bank.length;
        int prev=0;
        int res=0;
        for(int i=0;i<n;i++){
            int curr=0;
            for(char ch:bank[i].toCharArray()){
            if(ch=='1'){
                curr++;
            }
            }
            if(curr!=0){
                res+=curr*prev;
                prev=curr;
            }
            
        }
        return res;
    }
}
