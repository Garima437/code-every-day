class Solution {
    public int longestBalanced(int[] nums) {
        int maxlen=0;
        int n=nums.length;
        for(int i=0;i<n;i++){
             HashSet<Integer> ev=new HashSet<>();
              HashSet<Integer> odd=new HashSet<>();
            for(int j=i;j<n;j++){
               if(nums[j]%2==0) ev.add(nums[j]);
               else odd.add(nums[j]);
               if(ev.size()==odd.size()){
                maxlen=Math.max(maxlen,j-i+1);
               }
            }
        }
        return maxlen;
    }
}
