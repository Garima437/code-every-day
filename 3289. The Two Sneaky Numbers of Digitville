class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        int n=nums.length;
        int[] ans=new int[2];
        HashMap<Integer,Integer> mp=new HashMap<>();
        for(int num:nums){
            mp.put(num,mp.getOrDefault(num,0)+1);
        }
        int x=0;
        for(Map.Entry<Integer,Integer> e:mp.entrySet()){
            if(e.getValue()>1){
                ans[x++]=e.getKey();
            }
        }
        return ans;
    }
}
