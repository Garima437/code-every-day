class Solution {
    public int minimumBoxes(int[] apple, int[] capacity) {
        int n=apple.length;
        int m=capacity.length;
        Arrays.sort(capacity);
        int sum=0;
        for(int i:apple){
            sum+=i;
        }
        int sumcpa=0;
        int count = 0;
        for(int i=m-1;i>=0;i--){
            sumcpa+=capacity[i];
            count++;
            if(sum<=sumcpa) return count;
        }
        return count;
    }
}
