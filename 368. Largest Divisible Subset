class Solution {
    public List<Integer> largestDivisibleSubset(int[] nums) {
        int n=nums.length;
        int[] dp=new int[n];
        int[] hash=new int[n];
        Arrays.fill(dp,1);
        Arrays.sort(nums);
        for(int i=0;i<n;i++){
            hash[i]=i;
            for(int j=0;j<i;j++){
                if(nums[i]%nums[j]==0&&dp[i]<dp[j]+1){
                    dp[i]=dp[j]+1;
                    hash[i]=j;
                }
            }
        }
        int maxlen=0,lastIdx=0;
        for(int i=0;i<n;i++){
            if(dp[i]>maxlen){
                maxlen=dp[i];
                lastIdx=i;
            }
        }
        List<Integer> lis=new ArrayList<>();
        while(hash[lastIdx]!=lastIdx){
            lis.add(nums[lastIdx]);
            lastIdx=hash[lastIdx];
        }
        lis.add(nums[lastIdx]);
        return lis;
    }
}
