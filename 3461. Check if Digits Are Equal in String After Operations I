class Solution {
    public boolean hasSameDigits(String s) {
        int n=s.length();
        while(n>2){
            StringBuilder ans=new StringBuilder();
            for(int i=0;i<n-1;i++){
                int a=s.charAt(i)-'0';
                int b=s.charAt(i+1)-'0';
                ans.append((a+b)%10);
            }
            s=ans.toString();
            n=s.length();
        }
        return s.charAt(0)==s.charAt(1);
    }
}
