class Solution {
    public int countPrimeSetBits(int left, int right) {
       
        int ans=0;
        for(int i=left;i<=right;i++){
            // int val=i;
            // int cnt=0;
            // while(val!=0){
            // int lsb = val&1;
            // if(lsb==1) cnt++;
            // val =val>>1;
            // }
            int cnt=Integer.bitCount(i);
          if(isPrime(cnt)) {
            ans++;
            
          }
         
        }
        return ans;
        
    }
     private boolean isPrime(int n){
        if(n < 2) return false;
        for(int i = 2; i * i <= n; i++){
            if(n % i == 0) return false;
        }
        return true;
    }
}
