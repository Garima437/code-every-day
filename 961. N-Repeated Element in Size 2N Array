class Solution {
    public int repeatedNTimes(int[] nums) {
        int n=nums.length;
        HashMap<Integer,Integer> mp =new HashMap<>();
        for(int i=0;i<n;i++){
            mp.put(nums[i],mp.getOrDefault(nums[i],0)+1);
        }
        int N=n/2;
        
        for(int key:mp.keySet()){
            if(mp.get(key)==N) return key;
        }
        return -1;
    }
}
